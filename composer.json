	"type": "wordpress-muplugin",
	"license": "MIT",
	"minimum-stability": "stable",
	"require": {
		"php": ">=8.0"
	},
	"autoload": {
		"psr-4": {
			"Eighteen73\\Orbit\\": "includes/classes/"
		}
	},
	"config": {
